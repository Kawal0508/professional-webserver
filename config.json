{
  "server": {
    "host": "0.0.0.0",
    "port": 8080,
    "max_connections": 500,
    "timeout": 5,
    "thread_pool_size": 100
  },
  "security": {
    "rate_limit": 1000,
    "max_file_size": 10485760,
    "allowed_methods": ["GET", "HEAD", "POST"],
    "cors_enabled": true,
    "cors_origins": ["*"]
  },
  "logging": {
    "level": "WARNING",
    "format": "%(asctime)s - %(levelname)s - %(message)s",
    "file": "webserver.log"
  },
  "cache": {
    "enabled": true,
    "max_size": 50000,
    "ttl": 7200
  },
  "ssl": {
    "enabled": false,
    "cert_file": null,
    "key_file": null
  }
}
